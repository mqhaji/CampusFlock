{
  "name": "express-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsx src/app.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate:fixtures": "tsx src/scripts/migrate-fixtures.ts"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "^4.22.1",
    "mongoose": "^8.21.0"
  },
  "devDependencies": {
    "@types/express": "4.17.1",
    "@types/mongoose": "5.10.4",
    "ts-node": "10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5.6.2"
  }
}
